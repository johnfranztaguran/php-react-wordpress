{
  "presets":[
    "@babel/preset-react",
    [ "@babel/preset-env", {
      "targets": {
        "browsers": "> 2%"
      }
    }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "babel-plugin-styled-components",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
  ]
}
